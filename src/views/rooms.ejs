<section id="rooms">

    <article class="rooms_section" id="general">
      <h1>Chat Rooms</h1>
      <ul class="discover_lineup">
            <!-- Used place holders -->
            <% let rooms = [{"title": "Chat 1" , "img" : "/images/chat.png" }, {"title": "Chat 2" , "img" : "/images/chat.png" },{"title": "Chat 3" , "img" : "/images/chat.png" }] %>
            <% rooms.forEach( item => { %>
                <li class="discover_item grow_hover shadow"><a href="/item/info">
                    <img src="<%= item.img %>" alt="<%= item.title %>">
                    <h1><%= item.title %></h1>
                </a></li>
            <% }) %>
  
      </ul>
    </article>
  
  </section>