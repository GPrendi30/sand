<% let pages=["home", "discover" , "friendlist" , "rooms", '', '',  ]; %>

    <div id="nav_buttons">
        <ul>
            <% pages.forEach( p=> { %>
                <% if (p == '') { %>
                    <li style="visibility: hidden">
                <% } else { %>
                    <li>
                <% } %>
                <a class="underline_animation" href="/<%= p %>">
                        <div class="nav-icon" id="<%= p %>"></div>
                        <h1>
                            <% let title=p.charAt(0).toUpperCase() + p.slice(1); %>
                                <%= title %>
                        </h1>
                    </a></li>
                <% }); %>
        </ul>

        <section id="options">
            <button class="nav-icon grow_hover" id="settings"></button>
            <button class="grow_hover nav-icon" id="minmax"></button>
        </section>
    </div>